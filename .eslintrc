{
  "extends": ["rackt", "plugin:react/recommended"],
  "globals": {
    "__DEV__": false
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "react/display-name": 0,
    "react/jsx-wrap-multilines": 2
  },
  "plugins": [
    "react"
  ],
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  }
}
